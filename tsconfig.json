{
    "compilerOptions": {
        "jsx":"react",
        "lib":["es6","dom"],
        "module": "commonjs",
        "target": "es5",
        "sourceMap":true,
        "moduleResolution": "node",
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noImplicitAny": true,
        "strictNullChecks": true
    },
    "include":["./src"],
    "exclude": [
        "node_modules",
        "build"
    ]
}